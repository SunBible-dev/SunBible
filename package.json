{
  "build": {
    "appId": "app.the-sunshining.sunbible",
    "productName": "SunBible",
    "win": {
      "publish": [
        "github"
      ],
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64",
            "ia32"
          ]
        }
      ]
    },
    "mac": {
      "target": [
        {
          "target": "dmg",
          "arch": [
            "x64",
            "ia32"
          ]
        }
      ]
    },
    "linux": {
      "target": [
        {
          "target": "deb",
          "arch": [
            "x64",
            "ia32"
          ]
        }
      ]
    }
  },
  "name": "sunbible",
  "version": "0.0.7",
  "description": "SunBible: A beautiful way to read, study, and memorize the Bible. KJV",
  "main": "main.js",
  "homepage": "https://the-sunshining.github.io/SunBible/",
  "license": "MIT",
  "maintainers": [
    "NCR",
    "The SunShining"
  ],
  "author": "The SunShining <thesunshining@gmail.com> (www.thesunshining.weebly.com)",
  "keywords": [
    "Bible",
    "SunBible"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/the-sunshining/SunBible.git"
  },
  "devDependencies": {
    "electron": "^11.0.0-beta.13",
    "electron-builder": "^22.9.1"
  },
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "deb": "electron-builder --linux deb",
    "AppImage": "electron-builder --linux AppImage",
    "snap": "electron-builder --linux snap",
    "apk": "electron-builder --linux apk",
    "appx": "electron-builder --windows appx",
    "nsis": "electron-builder --win nsis",
    "dmg": "electron-builder --mac dmg",
    "ebPublish": "electron-builder --publish onTagOrDraft"
  },
  "dependencies": {
    "electron-context-menu": "^2.3.0",
    "electron-updater": "^4.3.5"
  }
  
}
